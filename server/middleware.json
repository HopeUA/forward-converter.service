{
  "initial": {
    "./middlewares/HidePoweredBy": {}
  },
  "parse": {
    "body-parser#raw": {}
  },
  "routes": {
    "./middlewares/Status": {
      "methods": ["GET"],
      "paths": ["/"]
    },
    "./middlewares/Converter": {
      "methods": ["POST"],
      "paths": ["/v2/files"]
    }
  },
  "files": {
    "loopback#static": {
      "paths": "/data",
      "params": "$!../data"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
